    "test:all-days": "node src/test/reserve-test.js --test",
    "test:custom": "node src/test/reserve-test.js",
    "test:wednesday": "node src/test/reserve-test.js --day=wednesday",
    "test:thursday": "node src/test/reserve-test.js --day=thursday",
    "test:friday": "node src/test/reserve-test.js --day=friday",
    "test:saturday": "node src/test/reserve-test.js --day=saturday",
